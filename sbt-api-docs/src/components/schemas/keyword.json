        "type": "string",
        "nullable": true,
        "description": "Data"
      },
      "appCode": {
        "type": "string",
        "nullable": true,
        "example": "gen.1200",
        "description": "API response code (see table above)."
      },
      "message": {
        "type": "string",
        "nullable": true,
        "example": "Request successful",
        "description": "API response message (see table above)."
      },
      "isError": {
        "type": "boolean",
        "default": false,
        "description": "If no error, *false*. If error, *true*."
      }
    },
    "additionalProperties": false
  }
